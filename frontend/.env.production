# Production Environment Variables for PneumoNet Frontend

# API Configuration - Using Vercel proxy to avoid mixed content issues
# The API calls will be routed through Vercel's HTTPS proxy to Azure HTTP backend
# REACT_APP_API_URL is not needed in production as we use relative URLs for proxy

# App Configuration
REACT_APP_NAME=PneumoNet
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_MOCK_DATA=false

# Analytics (if needed in future)
REACT_APP_ANALYTICS_ID=

# Build Configuration
GENERATE_SOURCEMAP=false
CI=false
