{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://pneumonia-detection-sheryansh.centralindia.azurecontainer.io:5000/$1",
      "headers": {
        "x-forwarded-host": "pneumonet-frontend.vercel.app"
      }
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/health.js": {
      "runtime": "@vercel/node@3"
    },
    "api/predict.js": {
      "runtime": "@vercel/node@3"
    },
    "api/predict-proxy.js": {
      "runtime": "@vercel/node@3"
    },
    "api/proxy.js": {
      "runtime": "@vercel/node@3"
    },
    "api/test.js": {
      "runtime": "@vercel/node@3"
    },
    "api/hello.js": {
      "runtime": "@vercel/node@3"
    }
  }
}
